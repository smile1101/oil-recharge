{
  "name": "zlwleng/oil-recharge",
  "description": "oil-recharge-php for php",
  "keywords": [
    "recharge",
    "composer-plugin",
    "php",
    "oil",
    "ofpay",
    "gxpay"
  ],
  "homepage": "http://github.com/zlwleng/oil-recharge",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Peter",
      "email": "smile_1101@yeah.net"
    }
  ],
  "require": {
    "php": "^7.0",
    "guzzlehttp/guzzle": "~6.0",
    "symfony/http-foundation": "^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "1.0.x-dev"
  },
  "autoload": {
    "files": [
      "src/Supports/helpers.php"
    ],
    "psr-4": {
      "Recharge\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Recharge\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev"
}
